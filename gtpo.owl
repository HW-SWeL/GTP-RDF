                    rdfs:comment "isomeric SMILES, describing a ligand" .


###  http://rdf.guidetopharmacology.org/ns/gtpo/iupacName
gtpo:iupacName rdf:type owl:DatatypeProperty ;
               rdfs:domain gtpo:Ligand ;
               rdfs:range xsd:string ;
               rdfs:comment "name of a ligand, defined by the IUPAC" .


###  http://rdf.guidetopharmacology.org/ns/gtpo/ligandName
gtpo:ligandName rdf:type owl:DatatypeProperty ;
                rdfs:domain gtpo:Ligand ;
                rdfs:range xsd:string ;
                rdfs:comment "the name of a chemical ligand" .


###  http://rdf.guidetopharmacology.org/ns/gtpo/nomenclature
gtpo:nomenclature rdf:type owl:DatatypeProperty ;
                  rdfs:domain gtpo:Target ;
                  rdfs:range xsd:string ;
                  rdfs:comment "nomenclature, the name of a target" .


###  http://rdf.guidetopharmacology.org/ns/gtpo/synonym
gtpo:synonym rdf:type owl:DatatypeProperty ;
             rdfs:domain gtpo:Ligand ;
